{
    "name": "laravel-sail",
    "displayName": "laravel Sail",
    "description": "support laravel sail",
    "icon": "images/icon.png",
    "publisher": "shashraf",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.66.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shreifelagamy/vscode-laravel-sail"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/composer.json"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "laravelSail.refresh",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "larave-sail.startSail",
                "title": "Laravel Sail: Start"
            },
            {
                "command": "larave-sail.stopSail",
                "title": "Laravel Sail: Stop"
            },
            {
                "command": "larave-sail.routeList",
                "title": "Laravel Sail: Route list"
            },
            {
                "command": "larave-sail.share",
                "title": "Laravel Sail: Share"
            },
            {
                "command": "laravelSail.delete",
                "title": "Laravel Sail: Delete"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "laravel-sail",
                    "title": "Laravel Sail",
                    "icon": "images/logo-activitybar.png"
                }
            ]
        },
        "views": {
            "laravel-sail": [
                {
                    "id": "laravel-sail-main",
                    "name": "Dashboard",
                    "type": "webview"
                },
                {
                    "id": "laravel-sail-containers",
                    "name": "containers",
                    "type": "tree"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "laravelSail.refresh",
                    "group": "navigation"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Laravel Sail Configuration",
            "properties": {
                "laravelSail.phpPath": {
                    "type": "string",
                    "default": "php",
                    "description": "Path to the PHP executable"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "ts-loader": "^9.2.8",
        "typescript": "^4.5.5",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@vscode/codicons": "^0.0.36"
    }
}
